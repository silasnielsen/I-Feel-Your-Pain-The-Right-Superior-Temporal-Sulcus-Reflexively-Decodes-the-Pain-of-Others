MRIwrite(tmp_ExpPain1,'beta_pos_ExpPain1_masked_STS-ImpExp1_MASKED4.nii','float');
csvwrite('beta_pos_ExpPain1_masked_STS_ImpExp1_MASKED4.csv',vol_mask_ExpPain1)

